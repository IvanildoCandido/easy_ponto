{
  "mcpServers": {
    "supabase": {
      "url": "https://api.supabase.com",
      "command": "node",
      "args": [
        "-e",
        "require('@supabase/mcp-server-supabase')"
      ],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}",
        "SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}"
      }
    }
  }
}









